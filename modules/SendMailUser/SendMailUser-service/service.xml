<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="org.salvacomes">
	<namespace>SendMailUser</namespace>
	<entity name="RegistryUsers" local-service="true" remote-service="false" table="RegistryForm">
        <column name="id" type="int" primary="true"></column>
        <column name="name" type="String"></column>
        <column name="lastName" type="String"></column>
        <column name="birthday" type="Date"></column>
        <column name="email" type="String"></column>
        <column name="registryDate" type="Date"></column>
    </entity>
</service-builder>